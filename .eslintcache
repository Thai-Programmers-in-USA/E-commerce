  ${product.price.toFixed(2)}\n                    </Typography>\n                }\n                <Typography className={classes.finalPrice} variant=\"body2\" component=\"p\" gutterBottom>\n                    ${finalPrice}\n                </Typography>\n            </div>\n            <div className={classes.btnContainer}>\n                <Button onMouseOver={classes.delayBtn} className={classes.btnQuickShop} variant=\"outlined\">\n                    <Typography className={classes.btnQuickShopText} variant=\"h6\" component=\"p\">Quick shop</Typography>\n                </Button>\n                <Button className={classes.btnChooseOption} variant=\"contained\" color=\"primary\">\n                    <Typography className={classes.btnChooseOptionText} variant=\"h6\" component=\"p\">Choose options</Typography>\n                </Button>\n            </div>\n        </Card>\n    )\n}\n\nProductCard.propTypes = {\n    classes: PropTypes.object.isRequired,\n    product: PropTypes.object.isRequired\n};\n\n\nconst styles = ({palette,breakpoints}) => ({\n    container: {\n        display: \"flex\",\n        flexDirection: \"column\",\n        alignItems: \"space-between\",\n        justifyContent: \"space-between\",\n        padding: \"20px\",\n        \n        [breakpoints.up('md')]: {\n            \"&:hover $btnContainer\": {\n                display: \"flex\",\n            },\n        },\n        [breakpoints.down('sm')]: {\n            width: \"220px\",\n            height: \"450px\",\n            display: \"inline-flex\",\n        },\n    },\n    img: {\n        width: '240px',\n        alignSelf: \"center\",\n\n        [breakpoints.down('sm')]: {\n            width: \"160px\",\n        },\n    },\n    detailsContainer: {\n        display: \"flex\",\n        flexDirection: \"column\",\n        justifyContent: \"flex-start\",\n        width: \"100%\",\n        padding: \"5px\",\n\n        [breakpoints.down('sm')]: {\n            height: \"100%\",\n        },\n\n